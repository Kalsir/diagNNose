{
  "model": {
    "type": "ForwardLSTM",
    "state_dict": "../lm_data/models/gulordava/state_dict.pt"
  },
  "vocab": {
    "path": "../lm_data/models/gulordava/vocab.txt"
  },
  "corpus": {
    "path": "../lm_data/corpora/downstream/lakretz/nounpp.txt",
    "header": ["sen", "type", "correct", "idx"]
  },
  "activations": {
  },
  "init_states": {
    "use_default": true
  },
  "decompose": {
    "decomposer": "ContextualDecomposer",
    "rel_interactions": ["rel-rel", "rel-b", "rel-irrel"],
    "only_source_rel": true,
    "normalize": true,
    "decompose_o": true,
    "fix_shapley": false
  },
  "plot_attention": {
    "clim": [-0.26, 0.7],
    "value_font_size": 22
  }
}
